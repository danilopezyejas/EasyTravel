	<body class="style-2">
		<div class="container">
					<div class="row">
						<div class="col-md-4">
							<!-- Start Sign In Form -->
							<form action="#" class="fh5co-form animate-box" data-animate-effect="fadeInLeft">
								<h2>Registrarse</h2>
								<div class="form-group">
									<label for="name" class="sr-only">Nombre</label>
									<input type="text" class="form-control" id="name" placeholder="Nombre" autocomplete="off">
								</div>
								<div class="form-group">
									<label for="email" class="sr-only">Email</label>
									<input type="email" class="form-control" id="email" placeholder="Email" autocomplete="off">
								</div>
								<div class="form-group">
									<label for="password" class="sr-only">Password</label>
									<input type="password" class="form-control" id="password" placeholder="Password" autocomplete="off">
								</div>
								<div class="form-group">
									<label for="re-password" class="sr-only">Repita Password</label>
									<input type="password" class="form-control" id="re-password" placeholder="Repita Password" autocomplete="off">
								</div>
								<div class="form-group">
									<label for="remember"><input type="checkbox" id="remember"> Recuerdame</label>
								</div>
								<div class="form-group">
									<p>Ya esta registrado? <a href="login">Login</a></p>
								</div>
								<div class="form-group">
									<input type="submit" value="Guardar" class="btn btn-primary">
								</div>
							</form>

						</div>
					</div>
				</div>

