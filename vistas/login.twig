					<!-- Start Sign In Form -->
					<form action="#" class="fh5co-form animate-box" data-animate-effect="fadeInLeft">
						<h2>Login</h2>
						<div class="form-group">
							<label for="username" class="sr-only">Nickname</label>
							<input type="text" class="form-control" id="username" placeholder="Nickname o correo" autocomplete="off">
						</div>
						<div class="form-group">
							<label for="password" class="sr-only">Password</label>
							<input type="password" class="form-control" id="password" placeholder="Password" autocomplete="off">
						</div>
						<div class="form-group">
							<label for="remember"><input type="checkbox" id="remember"> Recuerdame</label>
						</div>
						<div class="form-group">
							<p>No estas registrado? <a href="registro">Registrarse</a> | <a href="#">Olvidaste tu password?</a></p>
						</div>
						<div class="form-group">
